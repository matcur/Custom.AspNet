@model IEnumerable<string>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
<div>
    <form action="/files" method="post" enctype="multipart/form-data">
        <input type="file" name="images" multiple/>
        <button type="submit">Submit</button>
    </form>

    @foreach (var img in Model)
    {
        <img src="@img" alt="Not found">
    }
</div>
</body>
</html>